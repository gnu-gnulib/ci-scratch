msgstr " * [-]iutf8      pretpostavi da su ulazni znakovi UTF-8 kodirani\n"

#: src/stty.c:721
msgid " * [-]iuclc      translate uppercase characters to lowercase\n"
msgstr " * [-]iuclc      pretvori velika slova u mala\n"

#: src/stty.c:726
